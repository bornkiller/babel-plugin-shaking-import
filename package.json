  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/huang-xiao-jian/babel-plugin-shaking-import.git"
  },
  "keywords": [
    "babel",
    "plugin"
  ],
  "author": "huang-xiao-jian <hjj491229492@hotmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/huang-xiao-jian/babel-plugin-shaking-import/issues"
  },
  "homepage": "https://github.com/huang-xiao-jian/babel-plugin-shaking-import#README",
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/jest": "^24.0.23",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-tester": "^8.0.0",
    "commitizen": "^4.0.3",
